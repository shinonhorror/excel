{
  "parser": "babel-eslint",
  "env": {
    "es6":true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi":"off",
    "arrow-parens":"off",
    "linebreak-style": "off",
    "max-len":"off",
    "comma-dangle":"off",
    "require-jsdoc":"off",
    "object-curly-spacing":"off"
  },
  "extends": ["eslint:recommended", "google"]
}